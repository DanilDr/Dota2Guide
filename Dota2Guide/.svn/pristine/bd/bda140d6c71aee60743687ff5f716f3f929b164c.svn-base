<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:mm="http://millennialmedia.com/android/schema"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    tools:context="ru.bravedevrus.dotaguidetactics.ShowWallpaper"
    tools:ignore="MergeRootFrame" >
    
    <com.danildr.cacheimageview.CacheImageView
        android:id="@+id/wpMainImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />

    <ru.bravedevrus.dotaguidetactics.RomulTextView
        android:id="@+id/saveImage"
        android:layout_width="match_parent"
        android:text="@string/saveimage"
        android:layout_height="@dimen/romultext_view_height"
        android:layout_marginBottom="110dp"
        android:layout_gravity="bottom"
        android:textColor="@color/mainbuttontextcolor"
        android:gravity="center"
        android:background="@color/mainbuttoncolor"
        android:textSize="@dimen/romultext_view_size" />

    <ru.bravedevrus.dotaguidetactics.RomulTextView
        android:id="@+id/attachTo"
        android:layout_width="match_parent"
        android:layout_height="@dimen/romultext_view_height"
        android:layout_marginBottom="60dp"
        android:text="@string/attachto"
        android:layout_gravity="bottom"
        android:textColor="@color/mainbuttontextcolor"
        android:gravity="center"
        android:background="@color/mainbuttoncolor"
        android:textSize="@dimen/romultext_view_size" />   
    
    <LinearLayout
        android:id="@+id/mmediaad"
        android:layout_width="match_parent"
        android:layout_height="@dimen/advertisement_height"
        android:layout_gravity="bottom|center_horizontal"
        android:orientation="vertical"
        android:gravity="center"
        android:background="@android:color/black">
    </LinearLayout>
    
</FrameLayout>
